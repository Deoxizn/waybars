  "group/group-power": {
    "orientation": "horizontal",
    "drawer": {
      "click-to-reveal": true,
      "transition-duration": 500,
      "children-class": "not-power",
      "transition-left-to-right": true,
    },
    "modules": [
      "custom/trayicon",
      "tray",
      "backlight",
      "bluetooth",
      "pulseaudio#output",
      "pulseaudio#input",
      "network",
      "power-profiles-daemon",
      "memory",
      "cpu",
    ],
  },

