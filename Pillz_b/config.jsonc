{
  "reload_style_on_change": true,
  "layer": "top",
  "position": "top",
  "mod": "dock",
  "margin-top": 1,
  "height": 10,
  "margin-bottom": 2,
  "modules-left": ["custom/omarchy", "clock", "mpris"],

  //"modules-center": ["hyprland/window"],
  "modules-center": [
    "custom/weather",
    "hyprland/workspaces",
    "custom/updatespacman",
    "custom/update",
    "idle_inhibitor",
