    "states": {
      "warning": 20,
      "critical": 10,
    },
  },
  "bluetooth": {
    "format": "<span color='#0082FC'>Ôäî</span>",
    "format-disabled": "<span color='#0082FC'>Û∞Ç≤</span>",
    "format-off": "<span color='#0082FC'>Û∞Ç≤</span>",
    "format-connected": "<span color='#0082FC'>Û∞Ç±</span>",
